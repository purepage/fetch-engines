{
  "name": "@purepageio/fetch-engines",
  "version": "0.6",
  "type": "module",
  "description": "A collection of configurable engines for fetching HTML content using fetch or Playwright.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\" \"examples/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"examples/**/*.ts\" \"*.{js,cjs,json,md}\"",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:live": "LIVE_NETWORK=1 vitest run test/live/*.test.ts",
    "prepare": "husky",
    "examples:hybrid-md": "node scripts/hybrid-md-dump.mjs",
    "simple-scraping": "tsx examples/simple-scraping.ts",
    "smart-scraping": "tsx examples/smart-scraping.ts",
    "ai-extraction": "tsx examples/ai-extraction.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.30",
    "ai": "^5.0.44",
    "axios": "^1.6.8",
    "node-html-parser": "^7.0.1",
    "p-queue": "^7.4.1",
    "playwright": "^1.43.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "turndown": "^7.2.0",
    "turndown-plugin-gfm": "^1.0.2",
    "user-agents": "^1.1.208",
    "uuid": "^11.1.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.0.0",
    "@types/turndown": "^5.0.5",
    "@types/user-agents": "^1.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^24.1.3",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.29.0",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/purepage/fetch-engines"
  },
  "keywords": [
    "fetch",
    "scrape",
    "playwright",
    "headless",
    "browser",
    "automation",
    "engine"
  ],
  "author": "Purepage",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/purepageio/fetch-engines/issues"
  },
  "homepage": "https://github.com/purepageio/fetch-engines#readme",
  "publishConfig": {
    "access": "public"
  }
}
